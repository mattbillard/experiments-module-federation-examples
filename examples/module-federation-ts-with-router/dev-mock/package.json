{
  "name": "@module-federation-ts-with-router/dev-mock",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "webpack-cli serve",
    "clean": "rm -rf dist",
    "postinstall": "npm run clean && npm run link",
    "link": "mkdir -p dist/assets && npm run _link:app1 && npm run _link:app2 && npm run _link:site",
    "_link:app1": "cd dist/assets && ln -sf ../../../app1/dist app1",
    "_link:app2": "cd dist/assets && ln -sf ../../../app2/dist app2",
    "_link:site": "cd dist/assets && ln -sf ../../../site/dist site"
  },
  "dependencies": {},
  "devDependencies": {
    "@webpack-cli/serve": "^1.5.2",
    "serve": "12.0.0",
    "webpack": "5.52.0",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.1.0"
  }
}
