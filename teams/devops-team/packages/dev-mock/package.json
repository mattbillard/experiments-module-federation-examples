{
  "name": "@company/devops-team__dev-mock",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "webpack-cli serve",
    "clean": "rm -rf dist",
    "deploy": "mkdir -p dist/assets && run-p _deploy:*",
    "_deploy:definitions": "  rm -rf dist/assets/definitions      && cp -r ../../../../node_modules/@company/example-team1__definitions/dist/assets   dist",
    "_deploy:app1": "         rm -rf dist/assets/app1             && cp -r ../../../../node_modules/@company/example-team1__app1/dist/assets          dist",
    "_deploy:app2": "         rm -rf dist/assets/app2             && cp -r ../../../../node_modules/@company/example-team1__app2/dist/assets          dist",
    "_deploy:site": "         rm -rf dist/assets/site             && cp -r ../../../../node_modules/@company/core-team__site/dist/assets              dist"
  },
  "dependencies": {
    "@company/core-team__site": "0.0.0",
    "@company/example-team1__app1": "0.0.0",
    "@company/example-team1__app2": "0.0.0",
    "@company/example-team1__definitions": "0.0.0"
  },
  "devDependencies": {
    "@webpack-cli/serve": "^1.5.2",
    "npm-run-all": "^4.1.5",
    "serve": "12.0.0",
    "webpack": "5.52.0",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.1.0"
  }
}
