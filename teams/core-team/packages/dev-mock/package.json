{
  "name": "@module-federation-ts-with-router/dev-mock",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "webpack-cli serve",
    "clean": "rm -rf dist",
    "deploy": "mkdir -p dist/assets && run-p _deploy:*",
    "_deploy:definitions": "  rm -rf dist/assets/definitions      && cp -r ../definitions/dist/assets   dist",
    "_deploy:app1": "         rm -rf dist/assets/app1             && cp -r ../app1/dist/assets          dist",
    "_deploy:app2": "         rm -rf dist/assets/app2             && cp -r ../app2/dist/assets          dist",
    "_deploy:site": "         rm -rf dist/assets/site             && cp -r ../site/dist/assets          dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@webpack-cli/serve": "^1.5.2",
    "npm-run-all": "^4.1.5",
    "serve": "12.0.0",
    "webpack": "5.52.0",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.1.0"
  }
}
